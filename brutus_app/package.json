{
  "name": "brutus-ssh",
  "version": "0.1.2",
  "repository": "github:chriskrez/BrutusSSH",
  "private": true,
  "build": {
    "appId": "gr.auth.csd.chriskreza",
    "files": [
      "src/api/**/*"
    ]
  },
  "publish": {
    "provider": "github",
    "owner": "Christina Kreza",
    "private": true,
    "releaseType": "release"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.2",
    "chart.js": "^2.9.4",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dateformat": "^4.5.1",
    "electron-is-dev": "^1.2.0",
    "express-fileupload": "^1.2.1",
    "file-system": "^2.2.2",
    "get-port": "^5.1.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native": "^0.64.1",
    "react-native-web": "^0.16.3",
    "react-scripts": "4.0.3",
    "sass": "^1.32.8",
    "wait-on": "^5.2.1",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "electron": "^10.3.0",
    "electron-builder": "^22.9.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-dev": "nodemon --watch ./src/api --exec \"electron . --debug\"",
    "electron-build": "electron-builder",
    "release": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && npm run electron-dev\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
